{
  "name": "yt-downloader",
  "productName": "yt-downloader",
  "version": "1.2.2",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "repository": {
    "type": "git",
    "url": "https://github.com/lafferjm/yt-downloader.git"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "postinstall": "patch-package"
  },
  "keywords": [],
  "author": {
    "name": "Justin Lafferty",
    "email": "lafferjm@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "yt_downloader",
            "platforms": [
              "windows"
            ]
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux"
          ]
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "lafferjm",
              "name": "yt-downloader"
            },
            "draft": false
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@electron-forge/cli": "^6.0.0-beta.47",
    "@electron-forge/maker-deb": "^6.0.0-beta.47",
    "@electron-forge/maker-rpm": "^6.0.0-beta.47",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.47",
    "@electron-forge/maker-zip": "^6.0.0-beta.47",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.47",
    "@electron-forge/publisher-github": "^6.0.0-beta.47",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.0",
    "electron": "7.1.7",
    "node-loader": "^0.6.0",
    "patch-package": "^6.2.0",
    "style-loader": "^0.23.1"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "electron-squirrel-startup": "^1.0.0",
    "ffmpeg-static": "^2.7.0",
    "fluent-ffmpeg": "^2.1.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "update-electron-app": "^1.5.0",
    "ytdl-core": "^1.0.3"
  }
}
